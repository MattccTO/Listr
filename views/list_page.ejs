<head>
  <% include ./partials/_header %>
  <link rel="stylesheet" href="../styles/listpage.css" type="text/css" />
  <script type="text/javascript" src="/scripts/displayLists.js"></script>
  <script type="text/javascript" src="/vendor/jquery-3.0.0.js"></script>
</head>

<body>
  <header>
    <% include ./partials/_nav %>
  </header>
  <div class="container">
    <div class="row">
      <div class="col-sm-6">
        <div class="list-container">
          <form action="" accept-charset="UTF-8" method="post">
            <div class="input-group">
              <input type="text" name="add" id="add" placeholder=" Add an item..." class="form-control">
              <span class="input-group-btn">
                <input type="submit" name="commit" value="Add" class="btn btn-secondary" data-disable-with="Search">
              </span>
            </div>
          </form>
          <ul class="list-group">
            <span id="user_id" data-id="<%=ejsTemplate.id%>"></span>
          </ul>
        </div>
      </div>
      <div class="col">
        <div class="details">
          <div class="summary">
          </div>
          <div class="cat-buttons">
          </div>
        </div>
      </div>
    </div>
</body>
